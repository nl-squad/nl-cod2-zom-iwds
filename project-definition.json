{
    "connection": {
        "address": "mynl.pl",
        "user": "ubuntu",
        "keyPath": "~/.ssh/nl"
    },
    "profiles": {
        "default": {
            "localDeploymentPath": "src",
            "remoteDeploymentPath": "~/cod2/servers/nl-cod2-zom-dev",
            "rsyncExclude": [
                ".DS_Store",
                "nl/000empty.iwd",
                "library",
                "nl/maps/mp/mp_*.gsc",
                "nl/blanco"
            ],
            "containerName": "nl-cod2-zom-dev",
            "restartPath": "~/cod2/servers/nl-cod2-zom-dev",
            "restartDockerCompose": "docker-compose.dev.yml",
            "cod2": {
                "port": 7,
                "rconPasswordPath": "~/Dev/nl/nl-cod2-zom-dev.rcon",
                "iwdsPath": "src/nl"
            }
        },
        "public": {
            "localDeploymentPath": "src",
            "remoteDeploymentPath": "~/cod2/servers/nl-cod2-zom",
            "rsyncExclude": [
                ".DS_Store",
                "nl/000empty.iwd",
                "library",
                "nl/maps/mp/mp_*.gsc",
                "nl/blanco"
            ],
            "containerName": "nl-cod2-zom",
            "restartPath": "~/cod2/servers/nl-cod2-zom",
            "restartDockerCompose": "docker-compose.zom.yml",
            "cod2": {
                "port": 28960,
                "rconPasswordPath": "~/Dev/nl/nl-cod2-zom.rcon",
                "iwdsPath": "src/nl"
            }
        }
    }
}
